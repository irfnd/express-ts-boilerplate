{
	"env": { "es2021": true, "node": true },
	"extends": ["eslint:recommended", "standard-with-typescript", "prettier"],
	"plugins": ["@typescript-eslint", "prettier"],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "tsconfig.json"
	},
	"rules": {
		"prettier/prettier": "off"
	}
}
